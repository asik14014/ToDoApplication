<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TodoData" namespace="TodoData.Models.User" >
  <class name="UserInfo" table="UsersInfo" >
    <id name="Id" column="bintId" type="Int64" generator="native" />
    <property name="PhotoUrl" column="vchPhotoUrl" type="string"/>
    <property name="Name" column="vchName" type="string"/>
    <property name="PhoneNumber" column="vchTelNumber" type="string"/>
    <property name="RawData" column="vchRawJson" type="string"/>
    <property name="Push" column="bitPush" type="Boolean"/>
    <property name="FavoritePushNotification" column="bitFavorite" type="Boolean"/>
    <property name="Notification" column="bitNotification" type="Boolean"/>
  </class>
</hibernate-mapping>