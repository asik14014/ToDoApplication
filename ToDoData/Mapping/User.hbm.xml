<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TodoData" namespace="TodoData.Models.User" >
  <class name="User" table="Users" >
    <id name="Id" column="bintId" generator="native" />
    <property name="UserName" column="vchEmail"/>
    <property name="PasswordHash" column="vchPassword"/>
    <property name="IsActive" column="bitIsActive"/>
    <property name="UserType" column="intUserType"/>
    <property name="UserInfoId" column="bintUserInfoId"/>
    <property name="AccountPlanId" column="intAccountPlan"/>
    <property name="Registration" column="dtRegistration"/>
    <property name="LastUpdate" column="dtLastUpdate"/>
  </class>
</hibernate-mapping>